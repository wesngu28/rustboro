// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Display {
  id        Int       @id
  name      String
  art       String
  sprite    String
  abilities String[]
  stats     String[]
  types     String[]
  title     String
  blurb     String
  varieties String[]
  evolution String[]
}